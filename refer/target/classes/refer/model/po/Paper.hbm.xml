<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="refer.model.po.Paper" table="paper" schema="wos_refer">
        <id name="id" column="id"/>
        <property name="title" column="title"/>
        <property name="authors" column="authors"/>
        <property name="reportId" column="report_id"/>
        <property name="expiryTime" column="expiry_time"/>
        <property name="finished" column="finished"/>
    </class>
</hibernate-mapping>